{
    "author":"ovgamesdev@gmail.com",
    "content_scripts": [
    {
        "css": ["index.css"],
        "js": ["lib/jquery/jquery.min.js", "lib/jquery/jquery.timeago.js", "lib/dayjs/dayjs.min.js", "init.js"],
        "matches": ["https://wasd.tv/*"]
    }],
    "description": "Некоторые настройки для wasd.tv",
    "icons":
    {
        "128": "img/icon128.png",
        "16": "img/icon16.png",
        "48": "img/icon48.png"
    },
    "manifest_version": 2,
    "name": "BetterWASD",
    "web_accessible_resources": [
        "img/*.png"
    ],
    "minimum_chrome_version": "41",
    "permissions": ["storage", "*://*.wasd.tv/*"],
    "version": "1.2.6"
}